{
  "manifest_version": 3,
  "name": "GitHub PR Title WeeklyFetcher",
  "version": "1.0",
  "description": "Fetch PR titles from GitHub repositories",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://github.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 